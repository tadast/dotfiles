[user]
  name = Tadas Tamosauskas
  email = tadastamo@gmail.com

[color]
  ui = true
[alias]
  st = status
  br = branch
  cm = checkout master
  wip = commit -m 'wip [ci skip]'
[core]
  excludesfile = ~/.gitignore

[format]
  pretty=format:%C(yellow)%h%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset

[init]
  templatedir = ~/.git-template
[web]
  browser = firefox

[github]
  user = tadast

[diff]
  algorithm = patience

[push]
  default = simple

[credential]
	helper = store
[pull]
	rebase = true
